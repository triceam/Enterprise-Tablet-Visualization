<?xml version="1.0" encoding="utf-8"?>
<swiz:Swiz xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:swiz="http://swiz.swizframework.org" xmlns:business="business.*" xmlns:model="model.*"  >
	
	
	<swiz:loggingTargets>
		<fx:Array>
			<swiz:SwizTraceTarget id="myTraceTarget"/>
		</fx:Array>
	</swiz:loggingTargets>
	
	<swiz:config>
		<swiz:SwizConfig 
			eventPackages="events.*"
			viewPackages="views.*,views.components.*"
			defaultDispatcher="global" />
	</swiz:config>
	
	
	<swiz:beanProviders>
		<business:ServiceBeans />
		<business:LogicBeans />
		<model:ModelBeans />
	</swiz:beanProviders>
	
</swiz:Swiz>
